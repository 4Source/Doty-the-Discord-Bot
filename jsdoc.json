{
    "source": {
        "include": [
            "src",
            "types"
        ],
        "exclude": "(node_modules/|docs)",
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "encoding": "utf8",
        "recurse": true,
        "destination": "./docs/",
        "readme": "./README.md"
    }
}